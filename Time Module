>>> time.asctime()
'Wed May 27 16:10:45 2020'
>>> tup = (2000,10,15,8,45,12,6,0,0)
>>> time.asctime(tup)
'Sun Oct 15 08:45:12 2000'
>>> 



>>> time.localtime()
time.struct_time(tm_year=2020, tm_mon=5, tm_mday=27, tm_hour=16, tm_min=27, tm_sec=27, tm_wday=2, tm_yday=148, tm_isdst=0)
>>> t = time.localtime()
>>> year = t[0]
>>> day = t[2]
>>> month = t[1]
>>> print(year)
2020
>>> day
27
>>> month
5
>>> print(str(day) + "/" + str(month) +"/" + str(year))
27/5/2020
